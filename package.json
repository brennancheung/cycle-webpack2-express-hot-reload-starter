{
  "name": "cycle-webpack2-hot-reload-starter",
  "version": "1.0.0",
  "description": "A starter template for Cycle.js with Webpack2 and Hot Reloading",
  "main": "index.js",
  "scripts": {
    "build": "NODE_ENV=production webpack --config webpack.config.js",
    "dev": "NODE_ENV=development webpack --config webpack.config.js",
    "start": "node src/server/server.js",
    "test": "jest --watch"
  },
  "keywords": [
    "cycle.js",
    "hot-reload",
    "starter",
    "boilerplate",
    "webpack2"
  ],
  "author": "Brennan Cheung",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "6.18.2",
    "babel-eslint": "7.1.1",
    "babel-loader": "6.2.9",
    "babel-polyfill": "6.16.0",
    "babel-preset-es2015": "6.18.0",
    "babel-preset-stage-0": "6.16.0",
    "css-loader": "0.26.1",
    "eslint": "3.11.1",
    "eslint-config-standard": "6.2.1",
    "eslint-plugin-import": "2.2.0",
    "eslint-plugin-standard": "2.0.1",
    "express": "4.14.0",
    "html-webpack-plugin": "2.24.1",
    "jest": "17.0.3",
    "style-loader": "0.13.1",
    "stylus": "0.54.5",
    "stylus-loader": "2.4.0",
    "webpack": "2.1.0-beta.27",
    "webpack-dev-middleware": "1.8.4",
    "webpack-hot-middleware": "2.13.2"
  },
  "jest": {
    "scriptPreprocessor": "<rootDir>/node_modules/babel-jest"
  },
  "dependencies": {
    "@cycle/dom": "14.2.0",
    "@cycle/xstream-run": "4.1.0",
    "xstream": "9.0.0"
  }
}
